{
  "name": "stratosys-jobs-service",
  "version": "1.0.0",
  "description": "Background job processing engine for Stratosys using BullMQ + Redis",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "worker": "ts-node-dev --respawn --transpile-only src/worker.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "bull-board": "^4.19.1",
    "bullmq": "^4.8.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.1",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0",
    "prom-client": "^14.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.11.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0"
  }
}
